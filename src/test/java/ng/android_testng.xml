<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="AndroidNativeSuite" parallel="tests" thread-count="2">
    <test name="Android_0">
        <parameter name="platform" value="android"/>
        <parameter name="properties" value="android.properties"/>
        <classes>
            <class name="signIn.android.SignInTest">
                <methods>
                    <include name="testLogin"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Android_1">
        <parameter name="platform" value="android"/>
        <parameter name="properties" value="android_1.properties"/>
        <classes>
            <class name="signIn.android.SignInTest">
                <methods>
                    <include name="testLoginAndLogout"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>